NaprawiÄ‡ (?)
f(a,b,)



<GDecl> ";" => Stmt::Decl(<>),

GStmt: Stmt = {
    ";" => Stmt::BStmt(vec![]),
    <GExpr> ";" => Stmt::EStmt(<>),
    <GBlock> => Stmt::BStmt(<>),
    <GDecl> ";" => Stmt::Decl(<>),
    <Ident> "=" <Expr> ";" => Stmt::Ass(<>),
    <Ident> "++" ";" => Stmt::Incr(<>),
    <Ident> "--" ";" => Stmt::Decr(<>),
    "return" <Expr> ";" => Stmt::Ret(<>),
    "return" ";" => Stmt::VRet,
    "if" "(" <Expr> ")" <Stmt> => Stmt::Cond(<>),
    "if" "(" <Expr> ")" <Stmt> "else" <Stmt> => Stmt::CondElse(<>),
    "while" "(" <Expr> ")" <Stmt> => Stmt::While(<>),
}
