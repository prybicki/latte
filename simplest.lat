/**/
int fact1(int n) {
    if (n == 1) {
        return 1;
    }    
    return fact1(n + -1) * n;
}


int fact2(int n) {
    if (n == 1) {
        return 1;
    }
    else {
        return fact2(n + -1) * n;
    }
}


